<?xml version="1.0" encoding="UTF-8"?>
<ContentView
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="SampleApp.ViewCells.CustomFieldViewCell"
    x:Name="customFieldViewCell">
    <ContentView.Content>
        <StackLayout
            x:Name="slParentView">
            <Label
                Text="{Binding Label}"
                Style="{StaticResource Font17WithDarkGreyTextColorStyle}" />
        </StackLayout>
    </ContentView.Content>
</ContentView>
